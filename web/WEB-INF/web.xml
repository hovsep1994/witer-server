<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

    <servlet>
        <servlet-name>home</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.waiter.server.servlets.RegisterCompanyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/venues/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>cities</servlet-name>
        <servlet-class>com.waiter.server.servlets.SearchCitiesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cities</servlet-name>
        <url-pattern>/cities</url-pattern>
    </servlet-mapping>
</web-app>
